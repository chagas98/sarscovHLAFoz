@article{Francis:2021,
	title = {Allelic variation in class {I} {HLA} determines {CD8}+ {T} cell repertoire shape and cross-reactive memory responses to {SARS}-{CoV}-2},
	volume = {7},
	url = {https://www.science.org/doi/10.1126/sciimmunol.abk3070},
	doi = {10.1126/sciimmunol.abk3070},
	abstract = {Effective presentation of antigens by human leukocyte antigen (HLA) class I molecules to CD8+ T cells is required for viral elimination and generation of long-term immunological memory. In this study, we applied a single-cell, multiomic technology to generate a unified ex vivo characterization of the CD8+ T cell response to severe acute respiratory syndrome coronavirus 2 (SARS-CoV-2) across four major HLA class I alleles. We found that HLA genotype conditions key features of epitope specificity, TCRα/β sequence diversity, and the utilization of pre-existing SARS-CoV-2–reactive memory T cell pools. Single-cell transcriptomics revealed functionally diverse T cell phenotypes of SARS-CoV-2–reactive T cells, associated with both disease stage and epitope specificity. Our results show that HLA variations notably influence the CD8+ T cell repertoire shape and utilization of immune recall upon SARS-CoV-2 infection.},
	number = {67},
	urldate = {2024-06-16},
	journal = {Science Immunology},
	author = {Francis, Joshua M. and Leistritz-Edwards, Del and Dunn, Augustine and Tarr, Christina and Lehman, Jesse and Dempsey, Conor and Hamel, Andrew and Rayon, Violeta and Liu, Gang and Wang, Yuntong and Wille, Marcos and Durkin, Melissa and Hadley, Kane and Sheena, Aswathy and Roscoe, Benjamin and Ng, Mark and Rockwell, Graham and Manto, Margaret and Gienger, Elizabeth and Nickerson, Joshua and {MGH COVID-19 Collection and Processing Team} and Moarefi, Amir and Noble, Michael and Malia, Thomas and Bardwell, Philip D. and Gordon, William and Swain, Joanna and Skoberne, Mojca and Sauer, Karsten and Harris, Tim and Goldrath, Ananda W. and Shalek, Alex K. and Coyle, Anthony J. and Benoist, Christophe and Pregibon, Daniel C.},
	month = nov,
	year = {2021},
	
	pages = {eabk3070},
	file = {Full Text PDF:/home/samuel/Zotero/storage/CXHAEFJ9/Francis et al. - 2021 - Allelic variation in class I HLA determines CD8+ T.pdf:application/pdf},
}

