	keywords = {COVID-19, epidemiology, Health Status Disparities, Indicators of Morbidity and Mortality, Vaccination Coverage},
	pages = {105},
	file = {Full Text PDF:/home/samuel/Zotero/storage/4I6F8TJM/Moura et al. - 2022 - Covid-19 temporal evolution and immunization in t.pdf:application/pdf},
