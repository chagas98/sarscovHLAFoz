@article{Rivas:2020,
	title = {Increase {SARS}-{CoV}-2 infection in a border region after {Brazil} national holidays / {Aumento} da infecção de {SARS}-{CoV}-2 em uma região de fronteira após feriados nacionais no {Brasil}},
	volume = {6},
	issn = {2525-8761},
	url = {https://ojs.brazilianjournals.com.br/ojs/index.php/BRJD/article/view/22055},
	doi = {10.34117/bjdv6n12-571},
	abstract = {In recent years, the emergence and resurgence of infectious diseases have raised many questions about the role of epidemiological surveillance and understanding the patterns of infections can help in preventing and combating the disease. The objectives of this study were to report the increase in the number of positive cases in the border municipality of Foz do Iguaçu-PR and region, before and after national holidays in Brazil and its implications for public health. After the holidays, the average number of tests remains, and an increase in the number of positive cases is observed between days 5th, 6th, and 7th, 4.93\% on Corpus Christi, 6.44\% on Mother's Day, 4.20\% on Father's Day, 10.43\% on Independence Day and 14.49\% on Our Lady of Aparecida’s Day. The data can be useful, assisting hospital managers and health teams in the planning, and coordination of hospitals, since a sudden increase in the number of contaminated patients can generate a saturation in care stations and hospital units. This research demonstrates that national holidays can influence the increase in positive cases for SARS-CoV-2, and there are indications for greater contamination over long holidays.},
	language = {pt},
	number = {12},
	urldate = {2024-06-16},
	journal = {Brazilian Journal of Development},
	author = {Rivas, Açucena Veleh and Fruehwirth, Marcelo and Silveira, Cleypson Vinicius and Fritz, Andressa Faria Rahyn and Batista, Aline Cristiane Cechinel Assing and Delai, Robson Michael},
	month = dec,
	year = {2020},
	pages = {101252--101259},
	file = {Full Text PDF:/home/samuel/Zotero/storage/UI7XZZ75/Rivas et al. - 2020 - Increase SARS-CoV-2 infection in a border region a.pdf:application/pdf},
}

@article{Viana:2021,
	title = {Infection in asymptomatic carriers of {SARS}-{CoV}-2 can interfere with the achievement of robust immunity on a population scale},
	volume = {102},
	issn = {1465-2099},
	url = {https://www.microbiologyresearch.org/content/journal/jgv/10.1099/jgv.0.001684},
	doi = {10.1099/jgv.0.001684},
	abstract = {The severe acute respiratory syndrome coronavirus 2 (SARS-CoV-2) continues to spread worldwide as a severe pandemic, and a significant portion of the infected population may remain asymptomatic. Given this, five surveys were carried out between May and September 2020 with a total of 3585 volunteers in the municipality of Foz do Iguaçu, State of Paraná, a triple border region between Brazil/Argentina/Paraguay. Five months after the first infection, volunteers were re-analysed for the production of IgG anti-Spike and anti-RBD-Spike, in addition to analyses of cellular immunity. Seroconversion rates ranged from 4.4 \% to a peak of 37.21 \% followed by a reduction in seroconversion to 21.1 \% in September, indicating that 25 \% of the population lost their circulating anti-SARS-CoV-2 antibodies 3 months after infection. Analyses after 5 months of infection showed that only 17.2 \% of people still had anti-RBD-Spike antibodies, however, most volunteers had some degree of cellular immune response. The strategy of letting people become naturally infected with SARS-CoV-2 to achieve herd immunity is flawed, and the first contact with the virus may not generate enough immunogenic stimulus to prevent a possible second infection.},
	number = {11},
	urldate = {2024-06-17},
	journal = {Journal of General Virology},
	author = {Viana, Kelvinson and Zarpelon, Luis and Leandro, Andre and Terencio, Maria and Lopes, Renata and Martins, Caroline and Silva, Isaak and Sibim, Alessandra and Marques, Fábio and da Silva, Rafael and Rivas, Açucena and Souza, Adrieli and dos-Santos, Angelo and Torres, Sara and Garcia, Maria and Giunchetti, Rodolfo and Chiba-de-Castro, Wagner},
	year = {2021},
	
	pages = {001684},
	file = {Full Text PDF:/home/samuel/Zotero/storage/RXRPJWHU/Viana et al. - 2021 - Infection in asymptomatic carriers of SARS-CoV-2 c.pdf:application/pdf},
}

