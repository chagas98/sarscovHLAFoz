	pages = {1792},
	file = {Guerini et al. - 2022 - HLA Allele Frequencies and Association with Severi.pdf:/home/samuel/Zotero/storage/FQPRGTMR/Guerini et al. - 2022 - HLA Allele Frequencies and Association with Severi.pdf:application/pdf},
